{
  "name": "electron-musicplayer",
  "productName": "Electunes",
  "version": "1.0.0-rc4",
  "description": "Simple MusicPlayer",
  "main": "src/index.js",
  "scripts": {
    "start": "chcp 65001 && electron-forge start",
    "make": "npx electron-builder --win --x64 --dir",
    "package": "npx electron-builder --win --x64"
  },
  "keywords": [
    "Electron",
    "MusicPlayer"
  ],
  "author": {
    "name": "Nich87"
  },
  "license": "MIT",
  "build": {
